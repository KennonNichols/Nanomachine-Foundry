    <NanomachineFoundry.NaniteModifications.NaniteModificationDef>
        <defName>THNMF_SiegebreakerPunch</defName>
        <label>siegebreaker punch</label>


        <modScalingDetails>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Constant</scalingStyle>
                <label>50 base damage</label>
                <amount></amount>
            </li>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Constant</scalingStyle>
                <label>30 second cooldown</label>
                <amount></amount>
            </li>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Linear</scalingStyle>
                <label>{0} damage, multiplied by cube root of melee skill</label>
                <scalingOperatorSymbol>+</scalingOperatorSymbol>
                <amount>2</amount>
            </li>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Bonus</scalingStyle>
                <label>Instinct inhibitor increases siegebreaker punch damage by +50%, but user can break their hand from punching.</label>
