    <NanomachineFoundry.NaniteModifications.NaniteModificationDef>
        <defName>THNMF_Truesight</defName>
        <label>truesight</label>
        <modScalingDetails>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Linear</scalingStyle>
                <label>{0}% psychic sensitivity</label>
                <scalingOperatorSymbol>+</scalingOperatorSymbol>
                <amount>10</amount>
            </li>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Constant</scalingStyle>
                <label> Deaf to psychic drones, psychic disruption, and hostile psycasts when 5 or more nanites are allocated.</label>
                <amount></amount>
            </li>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingDetail">
                <scalingStyle>Bonus</scalingStyle>
                <label> Not fooled by dark psychic invisibility. Immune to weak psychic hypnosis.</label>
                <amount></amount>
            </li>
        </modScalingDetails>
        <iconPath>UI/Administrations/Modifications/Modicon_Truesight</iconPath>


        <scalingStatOffsets>
            <li Class="NanomachineFoundry.NaniteModifications.ModificationScalingStatAffector">
                <affectedStat>PsychicSensitivity</affectedStat>
                <amount>0.1</amount>
                <scalingStyle>Linear</scalingStyle>
            </li>
        </scalingStatOffsets>


        <categoryDef>THNMF_Archite</categoryDef>
        <description>Rewires the host's mind into a more efficient format. Increases psychic sensitivity, but makes the host deaf to certain broad-range hostile psychic attacks and prevents hostile psycasts from targetting them. Also allows the host to see through psychic influences of archotech origin.</description>
        <workerClass>NanomachineFoundry.NaniteModifications.ModificationWorkers.ModificationWorker_Truesight</workerClass>
        <ticker>false</ticker>
    </NanomachineFoundry.NaniteModifications.NaniteModificationDef>
    
